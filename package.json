{
  "name": "ts-morph-react",
  "version": "1.0.2",
  "description": "A collection of transformers for ts-morph to refactor react code",
  "license": "MIT",
  "author": {
    "name": "Tobias Strebitzer",
    "email": "tobias.strebitzer@atomic.bi"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/atomicbi/ts-morph-react.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "prepack": "pnpm clean && pnpm build",
    "watch": "tsup --watch",
    "lint": "eslint 'src/**/*.ts' && tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@eslint/core": "^1.0.0",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@typescript-eslint/typescript-estree": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "node-estree": "^4.0.0",
    "prettier": "^3.7.4",
    "ts-morph": "^27.0.2",
    "typescript-eslint": "^8.50.1"
  },
  "devDependencies": {
    "@base-ui/react": "^1.0.0",
    "@types/node": "^25.0.3",
    "cli-highlight": "^2.1.11",
    "react": "^19.2.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.21.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  }
}
