{
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "vitest",
    "skipFiles": ["<node_internals>/**", "build/**"],
    "program": "${workspaceFolder}/node_modules/vitest/dist/cli.js",
    "args": ["run", "-t", "combined", "--update"],
    "console": "internalConsole",
    "outputCapture": "std"
  }]
}
